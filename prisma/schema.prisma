generator client {
  provider  = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model UserRead {
  id Int @id @default(autoincrement())
  author String
  authorId String
  kanji String
  read String
  highRater String[]
  createdAt DateTime @default(now())
}
